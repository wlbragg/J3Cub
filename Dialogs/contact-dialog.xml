<PropertyList>

    <name>contact-dialog</name>
    <layout>vbox</layout>
    <default-padding>1</default-padding>
    <resizable>false</resizable>
    <modal>false</modal>
    <draggable>true</draggable>
	<width>250</width>

  <group>
    <layout>table</layout>

    <text>
      <halign>left</halign>
      <label>0</label>
      <row>0</row>
      <col>0</col>
    </text>        
    <slider>
        <halign>left</halign>
        <row>0</row>
        <col>1</col>
        <name>0</name>
        <min>-900</min>
        <max>900</max>
        <step>1</step>
        <live>true</live>
        <property>/fdm/jsbsim/contact/unit[0]/z-position</property>
        <binding>
            <command>dialog-apply</command>
            <object-name>0</object-name>
        </binding>
    </slider>
    <text>
        <halign>left</halign>
        <row>0</row>
        <col>2</col>
        <property>/fdm/jsbsim/contact/unit[0]/z-position</property>
