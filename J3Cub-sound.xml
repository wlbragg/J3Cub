<?xml version="1.0"?>
 
<PropertyList>

    <!--params>
        <door>sim/multiplay/generic/float[0]</door>
        <window>sim/multiplay/generic/float[1]</window>
    </params-->
    
    <fx>

        <crank>
            <name>engstart</name>
            <path>Sounds/engstart.wav</path>
            <property>/engines/active-engine/cranking</property>
            <volume>
                <factor>0.7143</factor>
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
            </volume>
            <volume>
                <offset>0.4</offset>
            </volume>
        </crank>

        <crank>
            <name>crank</name>
            <mode>looped</mode>
            <path>Sounds/cranking.wav</path>
            <condition>
                <and>
                    <property>/engines/active-engine/cranking</property>
                    <not>
                        <property>/engines/active-engine/running</property>
                    </not>
                </and>
            </condition>
            <volume>
                <factor>0.7143</factor>
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
            </volume>
            <volume>
                <offset>0.5</offset>
            </volume>
        </crank>

        <crank>
            <name>cough</name>
            <path>Sounds/coughing.wav</path>
            <property>/engines/active-engine/running</property>
            <volume>
                <offset>0.3</offset>
            </volume>
        </crank>

        <crank>
            <name>killed</name>
            <path>Sounds/coughing.wav</path>
            <property>/sim/model/j3cub/sound/click-coughing-engine-sound</property>
            <position>
                <x>-1.0</x>
                <y>0.0</y>
                <z>0.0</z>
            </position>
            <volume>
                <factor>0.7143</factor>
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
            </volume>
            <volume>
                <offset>0.0592</offset>
            </volume>
            <reference-dist>5.0</reference-dist>
            <max-dist>200.0</max-dist>
        </crank>

        <engine>
            <name>engine</name>
            <mode>looped</mode>
            <path>Sounds/wasp.wav</path>
            <property>/engines/active-engine/running</property>
            <volume>
                <property>/engines/active-engine/mp-osi</property>
                <factor>0.00375</factor>
                <min>0.15</min>
                <max>0.5</max>
            </volume>
            <volume>
                <property>/orientation/alpha-deg</property>
                <type>log</type>
                <min>1.0</min>
                <max>1.5</max>
            </volume>
            <volume>
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
            </volume>
            <pitch>
                <property>/engines/active-engine/rpm</property>
                <factor>0.0004</factor>
                <min>0.005</min>
                <max>2.0</max>
                <offset>0.35</offset>
            </pitch>
        </engine>

        <rumble>
            <name>rumble</name>
            <mode>looped</mode>
            <path>Sounds/rumble.wav</path>
            <condition>
                <or>
                    <property>/gear/gear[0]/wow</property>
                    <property>/gear/gear[1]/wow</property>
                    <property>/gear/gear[2]/wow</property>
		            <property>/gear/gear[19]/wow</property>
                    <property>/gear/gear[20]/wow</property>
                    <property>/gear/gear[21]/wow</property>
   		            <property>/gear/gear[22]/wow</property>
                    <property>/gear/gear[29]/wow</property>
                </or>
                <!-- <property>/sim/current-view/internal</property> -->
            </condition>
            <volume>
                <property>/velocities/groundspeed-kt</property>
                <type>log</type>
                <factor>0.25</factor>
            </volume>
            <volume>
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
            </volume>
        </rumble>

        <rumble-scratch>
            <name>rumble-scratch</name>
            <mode>looped</mode>
            <path>Sounds/scratch.wav</path>
            <condition>
                <and>
                    <or>
                        <!-- Broken gear touching the ground -->
                        <property>/fdm/jsbsim/contact/unit[6]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[7]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[8]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[30]/WOW</property>

                        <!-- Tail strike -->
                        <property>/fdm/jsbsim/contact/unit[3]/WOW</property>

                        <!-- Wing strike -->
                        <property>/fdm/jsbsim/contact/unit[4]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[5]/WOW</property>

                        <!-- Fuselage strike -->
                        <property>/fdm/jsbsim/contact/unit[9]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[10]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[11]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[12]/WOW</property>

                        <!-- Floats strike -->
                        <property>/fdm/jsbsim/contact/unit[13]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[14]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[15]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[16]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[17]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[18]/WOW</property>

                        <!-- Ski strike -->
                        <property>/fdm/jsbsim/contact/unit[26]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[27]/WOW</property>
                        <property>/fdm/jsbsim/contact/unit[28]/WOW</property>
                    </or>
                    <property>/fdm/jsbsim/ground/solid</property>
                </and>
            </condition>
            <position>
                <x>0.5</x>
                <y>0.0</y>
                <z>-1.15</z>
            </position>
            <volume>
                <property>velocities/groundspeed-kt</property>
                <type>log</type>
                <factor>0.25</factor>
            </volume>
            <volume>
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
            </volume>
            <reference-dist>2.5</reference-dist>
            <max-dist>200.0</max-dist>
        </rumble-scratch>

        <squeal>
            <name>squeal</name>
            <path>Sounds/squeal.wav</path>
            <condition>
                <or>
                    <property>/gear/gear[0]/wow</property>
                    <property>/gear/gear[1]/wow</property>
                    <property>/gear/gear[2]/wow</property>
                    <property>/gear/gear[29]/wow</property>
                    <property>/gear/gear[19]/wow</property>
                    <property>/gear/gear[20]/wow</property>
                    <property>/gear/gear[21]/wow</property>
                    <property>/gear/gear[22]/wow</property>
                </or>
            </condition>
            <volume>
                <property>/velocities/speed-down-fps</property>
                <factor>0.5</factor>
                <max>1.0</max>
            </volume>
            <volume>
                <property>/velocities/airspeed-kt</property>
                <factor>0.01</factor>
                <max>1.0</max>
            </volume>
            <volume>
                <internal>dt_stop</internal>
                <factor>0.05</factor>
                <max>1.0</max>
            </volume>
            <volume>
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
            </volume>
            <pitch>
                <property>/velocities/airspeed-kt</property>
                <factor>0.0025</factor>
                <offset>1.2</offset>
            </pitch>
        </squeal>
 
        <pavement-roll>
            <name>pavement-roll</name>
            <mode>looped</mode>
            <path>Sounds/tires-rolling-asphalt.wav</path>
            <condition>
                <and>
                    <property>/fdm/jsbsim/gear/on-pavement</property>
                    <or>
                        <property>gear/gear[0]/wow</property>
                        <property>gear/gear[1]/wow</property>
                        <property>gear/gear[2]/wow</property>
                        <property>gear/gear[29]/wow</property>
                        <property>gear/gear[19]/wow</property>
                        <property>gear/gear[20]/wow</property>
                        <property>gear/gear[21]/wow</property>
                        <property>gear/gear[22]/wow</property>
                    </or>
                    <or>
                        <greater-than>
                            <property>/gear/gear[0]/rollspeed-ms</property>
                            <value>5</value>
                        </greater-than>
                        <greater-than>
                            <property>/gear/gear[1]/rollspeed-ms</property>
                            <value>5</value>
                        </greater-than>
                        <greater-than>
                            <property>/gear/gear[2]/rollspeed-ms</property>
                            <value>5</value>
                        </greater-than>
                    </or>
                </and>
            </condition>
            <position>
                <x>0.5</x>
                <y>0.0</y>
                <z>-1.15</z>
            </position>
            <volume>
                <property>velocities/groundspeed-kt</property>
                <type>log</type>
                <factor>0.25</factor>
            </volume>
            <volume>
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
            </volume>
            <reference-dist>2.5</reference-dist>
            <max-dist>200.0</max-dist>
        </pavement-roll>

        <gravel-roll>
            <name>gravel-roll</name>
            <mode>looped</mode>
            <path>Sounds/tires-rolling-gravel.wav</path>
            <condition>
                <and>
                    <not>
                        <property>/fdm/jsbsim/gear/on-pavement</property>
                    </not>
                    <or>
                        <property>gear/gear[0]/wow</property>
                        <property>gear/gear[1]/wow</property>
                        <property>gear/gear[2]/wow</property>
                        <property>gear/gear[29]/wow</property>
                        <property>gear/gear[19]/wow</property>
                        <property>gear/gear[20]/wow</property>
                        <property>gear/gear[21]/wow</property>
                        <property>gear/gear[22]/wow</property>
                    </or>
                    <or>
                        <greater-than>
                            <property>/gear/gear[0]/rollspeed-ms</property>
                            <value>5</value>
                        </greater-than>
                        <greater-than>
                            <property>/gear/gear[1]/rollspeed-ms</property>
                            <value>5</value>
                        </greater-than>
                        <greater-than>
                            <property>/gear/gear[2]/rollspeed-ms</property>
                            <value>5</value>
                        </greater-than>
                    </or>
                </and>
            </condition>
            <position>
                <x>0.5</x>
                <y>0.0</y>
                <z>-1.15</z>
            </position>
            <volume>
                <property>velocities/groundspeed-kt</property>
                <type>log</type>
                <factor>0.25</factor>
            </volume>
            <volume>
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
            </volume>
            <reference-dist>2.5</reference-dist>
            <max-dist>200.0</max-dist>
        </gravel-roll>

        <gear-screech>
            <name>gear-screech</name>
            <mode>looped</mode>
            <path>Sounds/tires-screech-asphalt.wav</path>
            <condition>
                <greater-than>
                    <property>/fdm/jsbsim/gear/gear-screech</property>
                    <value>1</value>
                </greater-than>
            </condition>
            <position>
                <x>0.5</x>
                <y>0.0</y>
                <z>-1.15</z>
            </position>
            <volume>
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
            </volume>
            <reference-dist>2.5</reference-dist>
            <max-dist>200.0</max-dist>
        </gear-screech>

        <gear-drag>
            <name>gear-drag</name>
            <mode>once</mode>
            <path>Sounds/tires-screech-gravel-oneshot.wav</path>
            <condition>
                <greater-than>
                    <property>/fdm/jsbsim/gear/gear-drag</property>
                    <value>1</value>
                </greater-than>
            </condition>
            <position>
                <x>0.5</x>
                <y>0.0</y>
                <z>-1.15</z>
            </position>
            <volume>
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
            </volume>
            <reference-dist>2.5</reference-dist>
            <max-dist>200.0</max-dist>
        </gear-drag>

        <flap>
            <name>flaps</name>
            <mode>in-transit</mode>
            <path>Sounds/flaplever.wav</path>
            <property>surface-positions/flap-pos-norm-sound</property>
            <position>
                <x>0.5</x>
                <y>0.0</y>
                <z>0.5</z>
            </position>
            <volume>
                <factor>0.8</factor>
            </volume>
            <reference-dist>10.0</reference-dist>
            <max-dist>20.0</max-dist>
        </flap>
 
         <wind-left>
            <name>wind-left</name>
            <mode>looped</mode>
            <path>Sounds/wind1.wav</path>
            <condition>
                <!-- Silence "Neither a condition nor a property specified" warning -->
                <true/>
            </condition>
            <position>
                <x>0.0</x>
                <y>1.5</y><!-- Position left of pilot -->
                <z>0.0</z>
            </position>
            <volume>
                <property>position/altitude-ft</property>
                <factor>-0.000015</factor>
                <offset>1.0</offset>
                <min>0.1</min>
                <max>1.0</max>
            </volume>
            <volume>
                <property>velocities/airspeed-kt</property>
                <factor>0.0015</factor>
                <min>0.03</min>
                <max>0.25</max>
            </volume>
            <volume>
                <factor>0.7</factor> <!-- Arbitrary factor to attenuate wind -->
                <property>/sim/model/j3cub/sound/volume-boost-window</property>
            </volume>
            <volume>
                <offset>-0.03</offset>
            </volume>
            <pitch>
                <property>velocities/airspeed-kt</property>
                <factor>0.0035</factor>
                <offset>1.25</offset>
            </pitch>
            <reference-dist>1.0</reference-dist>
            <max-dist>100.0</max-dist>
        </wind-left>

        <wind-right>
            <name>wind-right</name>
            <mode>looped</mode>
            <path>Sounds/wind1.wav</path>
            <condition>
                <!-- Silence "Neither a condition nor a property specified" warning -->
                <true/>
            </condition>
            <position>
                <x>0.0</x>
                <y>-1.5</y><!-- Position right of co-pilot -->
                <z>0.0</z>
            </position>
            <volume>
                <property>position/altitude-ft</property>
                <factor>-0.000015</factor>
                <offset>1.0</offset>
                <min>0.1</min>
                <max>1.0</max>
            </volume>
            <volume>
                <property>velocities/airspeed-kt</property>
                <factor>0.0015</factor>
                <min>0.03</min>
                <max>0.25</max>
            </volume>
            <volume>
                <factor>0.7</factor> <!-- Arbitrary factor to attenuate wind -->
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
            </volume>
            <volume>
                <offset>-0.03</offset>
            </volume>
            <pitch>
                <property>velocities/airspeed-kt</property>
                <factor>0.0035</factor>
                <offset>1.25</offset>
            </pitch>
            <reference-dist>1.0</reference-dist>
            <max-dist>100.0</max-dist>
        </wind-right>

        <engine-repair>
            <name>engine-repair</name>
            <type>avionics</type>
            <mode>once</mode>
            <path>Sounds/repair.wav</path>
            <condition>
                <property>/sim/model/j3cub/sound/click-engine-repair</property>
            </condition>
        </engine-repair>

        <switch>
            <name>click-parking-brake</name>
            <mode>once</mode>
            <path>Sounds/lever.wav</path>
            <condition>
                <property>/sim/model/j3cub/sound/click-parking</property>
            </condition>
            <position>
                <x>-0.3660</x>
                <y>0.2000</y>
                <z>-0.4500</z>
            </position>
            <reference-dist>0.2</reference-dist>
            <max-dist>5.0</max-dist>
        </switch>
        
        <switch>
            <name>click-float-rudder</name>
            <mode>once</mode>
            <path>Sounds/lever.wav</path>
            <condition>
                <property>/sim/model/j3cub/sound/click-rudder</property>
            </condition>
            <position>
                <x> 0.3939</x>
                <y>-0.342 </y>
                <z>-0.7205</z>
            </position>
            <reference-dist>0.2</reference-dist>
            <max-dist>5.0</max-dist>
        </switch>

       <right-door-opening>
            <name>right-door-opening</name>
            <mode>once</mode>
            <path>Sounds/door-opening.wav</path>
            <condition>
                <greater-than>
                    <property>/sim/model/door-positions/rightDoor/position-norm</property>
                    <value>0.4</value>
                </greater-than>
            </condition>
            <position>
                <x>0.75</x>
                <y>-0.6</y>
                <z>-0.35</z>
            </position>
            <reference-dist>0.2</reference-dist>
            <max-dist>50.0</max-dist>
        </right-door-opening>

        <right-door-closing>
            <name>right-door-closing</name>
            <mode>once</mode>
            <path>Sounds/door-closing.wav</path>
            <condition>
                <less-than>
                    <property>/sim/model/door-positions/rightDoor/position-norm</property>
                    <value>0.5</value>
                </less-than>
            </condition>
            <position>
                <x>0.75</x>
                <y>-0.6</y>
                <z>-0.05</z>
            </position>
            <reference-dist>0.2</reference-dist>
            <max-dist>50.0</max-dist>
        </right-door-closing>

        <window-opening-one>
            <name>window-opening-one</name>
            <mode>once</mode>
            <path>Sounds/window-opening-one.wav</path>
            <condition>
                <greater-than>
                    <property>/sim/model/door-positions/rightWindow/position-norm</property>
                    <value>0.0</value>
                </greater-than>
            </condition>
            <position>
                <x>0.75</x>
                <y>-0.6</y>
                <z>-0.35</z>
            </position>a
            <reference-dist>0.2</reference-dist>
            <max-dist>50.0</max-dist>
        </window-opening-one>

        <window-opening>
            <name>window-opening</name>
            <mode>once</mode>
            <path>Sounds/window-opening.wav</path>
            <condition>
                <greater-than>
                    <property>/sim/model/door-positions/rightWindow/position-norm</property>
                    <value>0.9</value>
                </greater-than>
            </condition>
            <position>
                <x>0.75</x>
                <y>-0.6</y>
                <z>-0.75</z>
            </position>a
            <reference-dist>0.2</reference-dist>
            <max-dist>50.0</max-dist>
        </window-opening>

        <window-closing>
            <name>window-closing</name>
            <mode>once</mode>
            <path>Sounds/window-closing.wav</path>
            <condition>
                <less-than>
                    <property>/sim/model/door-positions/rightWindow/position-norm</property>
                    <value>0.01</value>
                </less-than>
            </condition>
            <position>
                <x>0.75</x>
                <y>-0.6</y>
                <z>-0.35</z>
            </position>
            <reference-dist>0.2</reference-dist>
            <max-dist>50.0</max-dist>
        </window-closing>

        <pontoon-fast-left>
            <name>pontoon-fast-left</name>
            <mode>looped</mode>
            <path>Sounds/water-fast.wav</path>
            <condition>
                <greater-than>
                    <property>/sim/model/j3cub/sound/spray-wake-speed</property>
                    <value>5</value>
                </greater-than>
            </condition>
            <position>
                <x>0.0</x>
                <y>1.1</y>
                <z>-2.0</z>
            </position>
            <reference-dist>5.0</reference-dist>
            <max-dist>200.0</max-dist>
            <volume>
                <property>/sim/model/j3cub/sound/spray-wake-speed</property>
                <factor>0.03</factor>
                <offset>0.2</offset>
                <min>0.0</min>
                <max>1.0</max>
            </volume>
            <volume>
                <factor>1.142857142857143</factor> <!-- (1.0+0.6)/1.4 (offset = -0.6, max boost: 1.4) -->
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
                <offset>-0.6</offset>
            </volume>
        </pontoon-fast-left>

        <pontoon-fast-right>
            <name>pontoon-fast-right</name>
            <mode>looped</mode>
            <path>Sounds/water-fast.wav</path>
            <condition>
                <greater-than>
                    <property>/sim/model/j3cub/sound/spray-wake-speed</property>
                    <value>5</value>
                </greater-than>
            </condition>
            <position>
                <x>0.0</x>
                <y>-1.1</y>
                <z>-2.0</z>
            </position>
            <reference-dist>5.0</reference-dist>
            <max-dist>200.0</max-dist>
            <volume>
                <property>/sim/model/j3cub/sound/spray-wake-speed</property>
                <factor>0.03</factor>
                <offset>0.2</offset>
                <min>0.0</min>
                <max>1.0</max>
            </volume>
            <volume>
                <factor>1.142857142857143</factor> <!-- (1.0+0.6)/1.4 (offset = -0.6, max boost: 1.4) -->
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
                <offset>-0.6</offset>
            </volume>
        </pontoon-fast-right>

        <pontoons-in-water-left>
            <name>pontoons-in-water-left</name>
            <mode>looped</mode>
            <path>Sounds/water-still.wav</path>
            <condition>
                <greater-than>
                    <property>/sim/model/j3cub/sound/spray-wake-speed</property>
                    <value>0</value>
                </greater-than>
            </condition>
            <position>
                <x>0.0</x>
                <y>1.1</y>
                <z>-2.0</z>
            </position>
            <reference-dist>2.0</reference-dist>
            <max-dist>200.0</max-dist>
            <volume>
                <offset>-0.2</offset>
            </volume>
            <volume>
                <factor>1.142857142857143</factor> <!-- (1.0+0.6)/1.4 (offset = -0.6, max boost: 1.4) -->
                <property>/sim/model/j3cub/sound/volume-boost-door</property>
                <offset>-0.6</offset>
            </volume>
        </pontoons-in-water-left>

        <pontoons-in-water-right>
            <name>pontoons-in-water-right</name>
            <mode>looped</mode>
            <path>Sounds/water-still.wav</path>
            <condition>
                <greater-than>
                    <property>/sim/model/j3cub/sound/spray-wake-speed</property>
                    <value>0</value>
                </greater-than>
            </condition>
