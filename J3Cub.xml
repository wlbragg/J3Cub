	<flight_control name="FCS: j3cub">

		<channel name="Landing Gear">
			<!-- Disable gear movement if electrical voltage is too low -->
			<switch name="Gear Command">
				<default value="gear/gear-cmd-norm"/>
				<output>gear/gear-input-norm</output>
				<!-- Route back the (normalized) output of the <kinematic>  to its input in order to disable it. -->
				<test logic="AND" value="gear/gear-pos-norm">
					/sim/model/j3cub/lighting/amphibious eq 0
				</test>
			</switch>
			<kinematic name="Gr">
				<input>gear/gear-input-norm</input>
				<traverse>
					<setting>
						<position>0</position>
						<time>0 </time>
					</setting>
					<setting>
						<position>1</position>
						<time>5</time>
					</setting>
				</traverse>
				<output>gear/gear-pos-norm</output>
			</kinematic>

			<kinematic name="/controls/gear/water-rudder-down">
				<input>/controls/gear/water-rudder</input>
				<traverse>
					<setting>
						<position>0</position>
						<time>0 </time>
					</setting>
					<setting>
						<position>1</position>
						<time>1</time>
					</setting>
				</traverse>
			</kinematic>
		</channel>

		<channel name="Flaps">
			<!-- Use to disable flaps if broken -->
			<switch name="Flaps Command">
				<default value="fcs/flap-cmd-norm"/>
				<output>fcs/flap-input-norm</output>

				<!-- Route back the (normalized) output of the <kinematic>
                     to its input in order to disable it.
  
                <test logic="AND" value="fcs/flap-pos-norm">
                    controls/flight/flaps-broken eq 1
                </test>              -->
			</switch>

			<kinematic name="Flaps Control">
				<input>fcs/flap-input-norm</input>
				<traverse>
					<setting>
						<position>0</position>
						<time>0</time>
					</setting>
					<setting>
						<position>25</position>
						<time>.5</time>
					</setting>
					<setting>
						<position>50</position>
						<time>.5</time>
					</setting>
				</traverse>
				<output>fcs/flap-pos-deg</output>
			</kinematic>

			<aerosurface_scale name="Flap Position Normalizer">
				<input>fcs/flap-pos-deg</input>
				<domain>
					<min>0</min> <!-- Flaps actual minimum position -->
					<max>50</max> <!-- Flaps actual maximum position -->
				</domain>
				<range>
					<min>0</min> <!-- Flaps normalized minimum position -->
					<max>1</max> <!-- Flaps normalized maximum position -->
				</range>
				<output>fcs/flap-pos-norm</output>
			</aerosurface_scale>

			<kinematic name="Flaps Lever">
				<input>/controls/flight/flaps</input>
				<traverse>
					<setting>
						<position>0</position>
						<time>0</time>
					</setting>
					<setting>
						<position>1</position>
						<time>.5</time>
					</setting>
					<setting>
						<position>2</position>
						<time>1</time>
					</setting>
				</traverse>
				<output>/sim/model/pa-18/cockpit/flaps-lever</output>
			</kinematic>
		</channel>

	</flight_control>

