  <parameters>
    <texture n="2">
        <image>Aircraft/j3cub/Models/Effects/exterior/floats-normal.png</image>
        <filter>linear-mipmap-linear</filter>
        <wrap-s>repeat</wrap-s>
        <wrap-t>repeat</wrap-t>
        <internal-format>normalized</internal-format>
    </texture>
    <texture n="4">
        <image>Aircraft/j3cub/Models/Effects/exterior/floats-ice.png</image>
