<?xml version="1.0"?>
<PropertyList>

 <path>compass.ac</path>

     <effect>
        <inherits-from>Effects/interior/j3cub-flashlight</inherits-from>
        <object-name>Compass</object-name>
        <object-name>Rim</object-name>
        <object-name>box</object-name>
        <object-name>rect</object-name>
        <object-name>screw_1</object-name>
        <object-name>screw_2</object-name>
        <object-name>screw_3</object-name>
        <object-name>screw_4</object-name>
        <object-name>screw_5</object-name>
        <object-name>screw_6</object-name>
        <object-name>screw_7</object-name>
        <object-name>screw_8</object-name>
    </effect>

 <animation>
  <type>rotate</type>
  <object-name>Compass</object-name>
  <property>/instrumentation/magnetic-compass/indicated-heading-deg</property>
  <center>
   <x-m>-0.027</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

</PropertyList>


