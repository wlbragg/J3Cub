<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <path>RedDragonEnginePreHeater.ac</path>
    <nopreview/>

    <animation>
        <type>select</type>
        <object-name>hoselow</object-name>
        <condition>
          <or>
            <and>
                <not>
                    <equals>
                        <property>fdm/jsbsim/bushkit</property>
                        <value>2</value>
                    </equals>
                </not>
                <not>
                    <equals>
                        <property>fdm/jsbsim/bushkit</property>
                        <value>3</value>
                    </equals>
                </not>
            </and>
            <property>sim/model/preload</property>
          </or>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>hosehigh</object-name>
        <condition>
          <or>
            <equals>
                <property>fdm/jsbsim/bushkit</property>
                <value>2</value>
            </equals>
            <equals>
                <property>fdm/jsbsim/bushkit</property>
                <value>3</value>
            </equals>
            <property>sim/model/preload</property>
          </or>
        </condition>
    </animation>

</PropertyList>
