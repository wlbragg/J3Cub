<?xml version="1.0"?>
<PropertyList>

 <path>tach.ac</path>
 
  <params>
      <engine>
          <rpm>controls/flight/speedbrake</rpm>
      </engine>
  </params>

  <effect>
      <inherits-from>../../Effects/interior/j3cub-interior</inherits-from>
      <object-name>Face</object-name>
      <object-name>bezelinside</object-name>
      <object-name>bezeloutside</object-name>
      <object-name>screws</object-name>
  </effect>

  <effect>
      <inherits-from>../../Effects/interior/j3cub-flashlight</inherits-from>
      <object-name>Needle1</object-name>
  </effect>

  <effect>
      <inherits-from>Aircraft/J3Cub/Models/Effects/interior/interior-glass-reflection-panel-front</inherits-from>
      <object-name>glass</object-name>
  </effect>

 <animation>
  <type>rotate</type>
  <object-name>Needle1</object-name>
  <property alias="/params/engine/rpm"/>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>3000.0</ind>
    <dep>229</dep>
   </entry>
  </interpolation>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
    <x-m>-0.172</x-m>
    <y-m>-0.19</y-m>
    <z-m>-0.46</z-m>
  </center>
 </animation>

</PropertyList>


