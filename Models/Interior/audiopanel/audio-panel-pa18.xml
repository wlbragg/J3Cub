    <!-- Audio Panel -->
    <animation>
        <type>pick</type>
        <object-name>AP.L0</object-name>
        <action>
            <name>COM1 MIC Button</name>
            <button>0</button>
            <binding>
                <command>nasal</command>
                <script>
                    j3cub.click("dblclk");
                </script>
            </binding>
            <binding>
                <command>property-toggle</command>
                <property>/instrumentation/audio-panel/com1-mic-btn</property>
            </binding>
            <binding>
                <command>nasal</command>
                <script>
                    var power = getprop("/instrumentation/audio-panel/com1-mic-btn");
                    if (power) {
                        setprop("/instrumentation/comm/power-btn", 1);
                        setprop("/instrumentation/audio-panel/com1-btn", 1);
                        setprop("/instrumentation/comm/volume", 1);
                    }
                </script>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>com1-mic-btn</tooltip-id>
                <label>COM 1 MIC: %s</label>
                <mapping>on-off</mapping>
                <property>/instrumentation/audio-panel/com1-mic-btn</property>
            </binding>
        </hovered>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>AP.L0</object-name>
        <property>/instrumentation/audio-panel/com1-mic-btn</property>
        <factor>-0.001</factor>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
