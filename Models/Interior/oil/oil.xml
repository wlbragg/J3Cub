<?xml version="1.0"?>
<PropertyList>

 <path>oil.ac</path>

  <effect>
      <inherits-from>../../Effects/interior/j3cub-interior</inherits-from>
      <object-name>Face</object-name>
      <object-name>bezelinside</object-name>
      <object-name>bezeloutside</object-name>
      <object-name>Face2bottom</object-name>
      <object-name>Face2top</object-name>
      <object-name>screws</object-name>
  </effect>

  <effect>
      <inherits-from>../../Effects/interior/j3cub-flashlight</inherits-from>
      <object-name>Needle1</object-name>
      <object-name>Needle2</object-name>
  </effect>

  <effect>
      <inherits-from>Aircraft/J3Cub/Models/Effects/interior/interior-glass-reflection-panel-front</inherits-from>
      <object-name>glass</object-name>
  </effect>

 <animation>
  <type>rotate</type>
  <object-name>Needle1</object-name>
  <property>engines/active-engine/oil-temperature-degf</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>200</ind>
    <dep>60</dep>
   </entry>
  </interpolation>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>-0.17039</x-m>
   <y-m> 0.1899</y-m>
   <z-m>-0.42278</z-m>
  </center>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Needle2</object-name>
  <property>engines/active-engine/oil-pressure-psi</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>80</ind>
    <dep>60</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>-0.17039</x-m>
   <y-m> 0.18981</y-m>
   <z-m>-0.4966</z-m>
  </center>
 </animation>

</PropertyList>


