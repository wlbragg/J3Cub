<?xml version="1.0"?>
<PropertyList>

 <path>compass.ac</path>

   <effect>
      <inherits-from>../../Effects/interior/j3cub-interior</inherits-from>
      <object-name>Rim</object-name>
      <object-name>box</object-name>
      <object-name>rect</object-name>
      <object-name>screws</object-name>
  </effect>

  <effect>
      <inherits-from>../../Effects/interior/j3cub-flashlight</inherits-from>
      <object-name>compass</object-name>
  </effect>

  <effect>
      <inherits-from>Aircraft/J3Cub/Models/Effects/interior/interior-glass-reflection-panel-front</inherits-from>
      <object-name>glass</object-name>
  </effect>

 <animation>
  <type>rotate</type>
  <object-name>compass</object-name>
  <property>instrumentation/magnetic-compass/indicated-heading-deg</property>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
  <center>
    <x-m>-0.200024</x-m>
    <y-m>-0.000279</y-m>
    <z-m>-0.455798</z-m>
  </center>
 </animation>

</PropertyList>


