<?xml version="1.0"?>

<PropertyList>

  <name type="string">parking</name>

  <overlay>
    <sim>
	    <freeze>
		    <master type="bool">true</master>
		    <clock type="bool">0</clock>
	    </freeze>
	    <presets>
		    <speed-set type="string">knots</speed-set>
		    <airspeed-kt type="int">0</airspeed-kt>
		    <onground type="bool">true</onground>
		    <heading-deg type="double">90</heading-deg>
	    </presets>
	    <model>
		    <j3cub>
			    <brake-parking type="bool">1</brake-parking>
		    </j3cub>
	    </model>
    </sim>
    <fdm>
	    <jsbsim>
		    <running type="bool">0</running>
	    </jsbsim>
    </fdm>
    <consumables>
	    <fuel>
		    <tank n="0">
			    <selected type="bool">true</selected>
		    </tank>
		    <tank n="1">
			    <selected type="bool">true</selected>
		    </tank>
		    <tank n="2">
			    <selected type="bool">true</selected>
		    </tank>
	    </fuel>
    </consumables>
    <engines>
	     <active-engine>
		    <running type="bool">0</running>
		    <auto-start type="bool">1</auto-start>
		    <cranking type="bool">1</cranking>
	     </active-engine>
    </engines>
    <controls>
	    <flight>
		    <flaps type="double">0</flaps>
	    </flight>
	    <gear>
		    <gear-down type="bool">true</gear-down>
	    </gear>
	    <switches>
		    <magnetos type="int">3</magnetos>
		    <master-bat type="bool">1</master-bat>
		    <master-alt type="bool">1</master-alt>
		    <master-avionics type="bool">1</master-avionics>
		    <starter type="bool">1</starter>
	    </switches>
	    <engines>
        <engine n="0">
          <primer type="double">3</primer>
          <primer-lever type="bool">0</primer-lever>
        </engine>
        <current-engine>
          <throttle type="double">0.25</throttle>
          <starter type="bool">1</starter>
        </current-engine>
	    </engines>
    </controls>
      <position>
        <altitude-agl-ft type="int">1</altitude-agl-ft>
      </position>
  </overlay>

</PropertyList>
